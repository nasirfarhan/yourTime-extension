{
    "manifest_version": 3,
    "name": "Reading Time",
    "version": "1.0",
    "description": "Add reading time to chrome extension documentation articles",
    "content_scripts":[
        {
            "js": ["scripts/content.js"],
            "matches": [
                "<all_urls>"
            ] ,
            "run_at": "document_idle"
        }
    ]
}


